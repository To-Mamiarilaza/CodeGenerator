{
    "language": [
        "JAVA",
        "C#"
    ],
    "fileExtension": {
        "JAVA": ".java",
        "C#": ".cs"
    },
    "packaging": {
        "JAVA": "package {packageName};",
        "C#": "namespace {packageName};"
    },
    "imports": {
        "JAVA": "import {type};",
        "C#": "using {type};"
    },
    "extends": {
        "JAVA": "extends {type}",
        "C#": ": {type}"
    },
    "fields": {
        "JAVA": "{fieldAnnotation}\n {type} {fieldName};",
        "C#": "{fieldAnnotation}\n {type} {fieldName} { get; set; }"
    },
    "typeMapping": {
        "int4": {
            "JAVA": "Integer",
            "C#": "int"
        },
        "int": {
            "JAVA": "Integer",
            "C#": "int"
        },
        "double": {
            "JAVA": "Double",
            "C#": "double"
        },
        "float8": {
            "JAVA": "Double",
            "C#": "double"
        },
        "numeric": {
            "JAVA": "Double",
            "C#": "double"
        },
        "varchar": {
            "JAVA": "String",
            "C#": "string"
        },
        "date": {
            "JAVA": "Date",
            "C#": "DateTime"
        },
        "time": {
            "JAVA": "Time",
            "C#": "TimeSpan"
        },
        "datetime": {
            "JAVA": "Timestamp",
            "C#": "DateTime"
        },
        "timestamp": {
            "JAVA": "Timestamp",
            "C#": "DateTime"
        },
        "boolean": {
            "JAVA": "Boolean",
            "C#": "bool"
        },
        "bool": {
            "JAVA": "Boolean",
            "C#": "bool"
        }
    },
    "typeImport": {
        "JAVA": {
            "Integer": null,
            "Double": null,
            "String": null,
            "Date": "java.sql.Date",
            "Time": "java.sql.Time",
            "Timestamp": "java.sql.Timestamp",
            "Boolean": null,
            "List": "java.util.List"
        },
        "C#": {
            "int": null,
            "double": null,
            "string": null,
            "DateTime": "System",
            "TimeSpan": "System",
            "bool": null,
            "List": "System.Collections.Generic"
        }
    },
    "getterAndSetter": {
        "JAVA": "public {type} get{capitalFieldName}() {\n\treturn {fieldName};\n}\n\npublic void set{capitalFieldName}({type} {fieldName}) {\n\tthis.{fieldName} = {fieldName};\n}\n\n",
        "C#": null
    },
    "DAOCompatibility": {
        "JAVA": ["JPA", "MyDAO"],
        "C#": ["Entity"]
    },
    "DAOImports": {
        "JPA": ["jakarta.persistence.*"],
        "Entity": ["System.ComponentModel.DataAnnotations", "System.ComponentModel.DataAnnotations.Schema"],
        "MyDAO": ["generalisation.annotations.DBTable", "generalisation.annotations.DBField"]
    },
    "DAOAnnotations": {
        "class": {
            "JPA": "@Entity\n@Table(name = \"{tableName}\")",
            "Entity": "[Table(\"{tableName}\")]",
            "MyDAO": "@DBTable(name = \"{tableName}\", isAutoIncrement = true)"
        },
        "field": {
            "JPA": "{PKAnnotation}\n{FKAnnotation}\n@Column(name = \"{columnName}\")",
            "Entity": "{PKAnnotation}\n{FKAnnotation}\n[Column(\"{columnName}\")]",
            "MyDAO": "@DBField(name = \"{columnName}\" {PKAnnotation} {FKAnnotation})"
        }
    },
    "PKAnnotations": {
        "JPA": "@Id\n@GeneratedValue(strategy = GenerationType.IDENTITY)",
        "Entity": "[Key]\n[DatabaseGenerated(DatabaseGeneratedOption.Identity)]",
        "MyDAO": ", isPrimaryKey = true"
    },
    "FKAnnotations": {
        "JPA": "@ManyToOne\n@JoinColumn(name = \"{fkColumnName}\")",
        "Entity": "[ForeignKey(\"{fkColumnName}\")]",
        "MyDAO": ", isForeignKey = true"
    }
}